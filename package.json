{
  "name": "grix",
  "version": "0.1.0",
  "private": true,
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "jest.config.js",
  "scripts": {
    "debug": "vite --mode debug",
    "dev": "vite --mode dev",
    "staging": "vite --mode staging",
    "build": "tsc && vite build",
    "build:widgets": "tsc && vite build --config vite.widgets.config.ts",
    "serve": "vite preview",
    "test": "jest",
    "playwright:show-report": "pnpm exec playwright show-report",
    "playwright": "pnpm exec playwright test --ui",
    "playwright:e2e": "pnpm exec playwright test --ui -c playwright.e2e.config.ts",
    "playwright:stats": "pnpm exec playwright test tests/integrations/StatsPage.spec.ts --headed",
    "postinstall": "sed -i.bak 's/COMMIT_HASH_PLACEHOLDER/$(git rev-parse HEAD)/g; s/DATETIME_PLACEHOLDER/$(date -u +\"%Y-%m-%dT%H:%M:%SZ\")/g' src/utils/commitHash.ts && rm src/utils/commitHash.ts.bak",
    "prepare": "husky install",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx}'",
    "format": "pnpm run prettier:fix && pnpm lint --fix",
    "ai-guide": "pnpm format && pnpm build && pnpm prettier:fix && pnpm exec playwright test",
    "mcp-browser": "npx @agentdeskai/browser-tools-server"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.4",
    "@cowprotocol/widget-lib": "^0.14.0",
    "@emotion/react": "^11.14.0",
    "@fuul/sdk": "^4.11.5",
    "@mui/material": "^6.4.5",
    "@premia/v3-sdk": "^2.9.0",
    "@r2wc/react-to-web-component": "^2.0.3",
    "@reown/appkit": "^1.6.7",
    "@reown/appkit-adapter-solana": "^1.6.4",
    "@reown/appkit-adapter-wagmi": "^1.6.4",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.98.0",
    "@tanstack/react-query": "^5.62.3",
    "@tanstack/react-table": "^8.20.5",
    "@visx/network": "^3.12.0",
    "axios": "^1.7.9",
    "date-fns": "^3.6.0",
    "framer-motion": "^10.18.0",
    "github-markdown-css": "^5.8.1",
    "global": "^4.4.0",
    "lightweight-charts": "^4.2.2",
    "mixpanel-browser": "^2.60.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.15.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "recharts": "2.13.0-alpha.5",
    "remark-gfm": "^4.0.0",
    "viem": "^2.21.54",
    "wagmi": "^2.13.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/mixpanel-browser": "^2.50.2",
    "@types/node": "^16.18.121",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ],
    "*.{css,scss}": "stylelint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IGO-Group/interface.git"
